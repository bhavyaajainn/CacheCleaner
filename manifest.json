{
  "manifest_version": 3,
  "name": "CacheCleaner",
  "version": "1.0",
  "description": "Effortlessly clear your cache, cookies, and history with a single click.",
  "action": {
    "default_popup": "index.html",
    "default_icons": {
      "16": "icons/clean16.png",
      "32": "icons/clean32.png",
      "48": "icons/clean48.png",
      "128": "icons/clean128.png"
    }
  },
  "permissions": [
    "browsingData",
    "cookies",
    "history"
  ],
  "icons": {
      "16": "icons/clean16.png",
      "32": "icons/clean32.png",
      "48": "icons/clean48.png",
      "128": "icons/clean128.png"
    }
}
